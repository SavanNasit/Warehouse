<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/home_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <LinearLayout
        android:id="@+id/drawer_linear_layout_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/home_page_gray"
        android:orientation="vertical"
        android:weightSum="4">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="top"
            android:layout_marginTop="10dp"
            android:layout_weight="0.5"
            android:gravity="bottom|center"
            android:lines="2"
            android:text="Warehouse"
            android:textColor="@color/primary"
            android:textSize="13sp" />

        <TextView
            android:id="@+id/activity_drawer_warehouse_name"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_gravity="top"
            android:layout_weight="0.5"
            android:gravity="top|center"
            android:lines="2"
            android:text=""
            android:textColor="@color/black"
            android:textSize="16sp" />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="2">

            <LinearLayout
                android:id="@+id/home_running_orders_layout"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/white"
                android:orientation="vertical"
                android:weightSum="2">


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:paddingTop="20dp"
                    android:src="@drawable/ic_running_orders" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="top|center"
                    android:text="Running Orders"
                    android:textColor="#8D8D8D"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/home_manage_packages_layout"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@color/white"
                android:orientation="vertical"
                android:weightSum="2">


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:paddingTop="20dp"
                    android:src="@drawable/ic_manage_package" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="top|center"
                    android:text="Manage Packages"
                    android:textColor="#8D8D8D"
                    android:textSize="14sp" />
            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_2sdp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="2">

            <LinearLayout
                android:id="@+id/home_manage_gate_pass_layout"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="@color/white"
                android:orientation="vertical"
                android:weightSum="2">


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:paddingTop="20dp"
                    android:src="@drawable/ic_manage_gatepass" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="top|center"
                    android:text="Manage Gate Pass"
                    android:textColor="#8D8D8D"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/home_manage_consignment_layout"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/_2sdp"
                android:layout_weight="1"
                android:background="@color/white"
                android:orientation="vertical"
                android:weightSum="2">


                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:paddingTop="20dp"
                    android:src="@drawable/ic_manage_consignment" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_gravity="center"
                    android:layout_weight="1"
                    android:background="@color/white"
                    android:gravity="top|center"
                    android:text="Manage Consignment"
                    android:textSize="14sp" />


            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/home_receive_consignment_layout"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_marginTop="@dimen/_2sdp"
            android:layout_weight="1"
            android:orientation="vertical"
            android:weightSum="2">


            <ImageView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:paddingTop="20dp"
                android:src="@drawable/ic_receive_consignment" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:background="@color/white"
                android:gravity="top|center"
                android:text="Receive Consignment"
                android:textColor="@color/home_page_text_gray"
                android:textSize="14sp" />

            <!--    <TextView
                    android:layout_width="0dp"
                    android:layout_weight="1"                            android:layout_height="100dp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:textColor="@color/home_page_text_gray"
                    android:background="@color/white"
                    android:gravity="center"
                    android:textSize="14sp" />-->

        </LinearLayout>

    </LinearLayout>


</FrameLayout>